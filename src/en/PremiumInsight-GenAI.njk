<script>
  document.addEventListener('DOMContentLoaded', () => {
    // Ensure the DOM is fully loaded before capturing the URL
    const currentUrl = window.location.href;

    // Set the URL to the hidden input field
    const urlField = document.getElementById('pageUrl');
    if (urlField) {
      urlField.value = currentUrl;
    } else {
      console.error('pageUrl field not found.');
    }
  });
</script>


<script>
  document.getElementById("contact-form").addEventListener("submit", function (e) {
    e.preventDefault(); // Prevent form refresh
    const formData = new FormData(this);
    fetch("/send_premium.php", {
      method: "POST",
      body: formData,
    })
      .then((response) => {
        if (response.ok) {
          document.getElementById("success-message").style.display = "block";
          document.getElementById("error-message").style.display = "none";
          this.reset();
        } else {
          throw new Error("Network response was not ok.");
        }
      })
      .catch(() => {
        document.getElementById("success-message").style.display = "none";
        document.getElementById("error-message").style.display = "block";
      });
  });
</script>

<script>
  document.querySelectorAll('.tabs-menu li').forEach(tab => {
    tab.addEventListener('click', () => {
      // Remove active class from all tabs
      document.querySelectorAll('.tabs-menu li').forEach(tab => tab.classList.remove('active-tab'));
      // Add active class to clicked tab
      tab.classList.add('active-tab');
      
      // Hide all tab contents
      document.querySelectorAll('.tab-content').forEach(content => content.classList.remove('active-content'));
      // Show active tab content
      const tabContentId = tab.getAttribute('data-tab');
      document.getElementById(tabContentId).classList.add('active-content');
    });
  });
</script>
